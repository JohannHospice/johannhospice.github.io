<% if(education != null) { %>
    <div uk-grid>
        <div class="uk-width-1-1 uk-width-medium-2-5">
            <h3 class="uk-h4 uk-margin-remove-bottom"><%= education.title %><br><%= education.place %></h3>
            <span class="tm-text-lead uk-display-block uk-text-muted"><%= education.date_begin %> - <%= education.date_end %></span>
        </div>
        <div class="uk-width-1-1 uk-width-medium-3-5">
            <p class="uk-text-muted"><%= education.description %></p>
        </div>
    </div>
<% } %>
